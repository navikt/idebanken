    "bugs": {
        "url": "https://github.com/navikt/idebanken/issues"
    },
    "homepage": "https://idebanken.org",
    "engines": {
        "node": ">= 20.10.0",
        "npm": ">= 10.2.3"
    },
    "dependencies": {
        "@babel/generator": "^7.27.5",
        "@babel/parser": "^7.27.5",
        "@babel/traverse": "^7.27.4",
        "@babel/types": "^7.27.6",
        "@enonic-types/guillotine": "^7.2.0"
    }
}
