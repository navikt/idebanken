<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part xmlns="urn:enonic:xp:model:1.0">
    <display-name>Innholdsfortegnelse</display-name>
    <description>Innholdsfortegnelse for "kort" layout på siden</description>
    <form>
        <input type="TextLine" name="title">
            <label>Tittel på innholdsfortegnelse</label>
            <default>Denne siden hjelper deg til å:</default>
        </input>
        <input name="sticky" type="CheckBox">
            <label>Sticky</label>
            <default>checked</default>
        </input>
    </form>
</part>
