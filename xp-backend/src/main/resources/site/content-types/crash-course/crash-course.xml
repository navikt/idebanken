<?xml version="1.0" encoding="UTF-8"?>
<content-type>
    <display-name>Lynkurs</display-name>
    <description>Powerpoint-aktig lynkurs</description>
    <display-name-expression>${title}</display-name-expression>
    <super-type>base:structured</super-type>
    <is-final>true</is-final>
    <allow-child-content-type>(?!idebanken:category$)${app}:crash-course-slide</allow-child-content-type>
    <form>
        <mixin name="title-ingress"/>
    </form>
</content-type>
