<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mixin xmlns="urn:enonic:xp:model:1.0">
    <form>
        <input name="displayType" type="ComboBox">
            <label>Visning</label>
            <occurrences minimum="1" maximum="1"/>
            <config>
                <option value="none">Uten ikon og bilde</option>
                <option value="withIcon">Med ikon</option>
                <option value="withImageAndIcon">Med ikon og bilde</option>
                <option value="withImage">Med bilde</option>
            </config>
            <default>withIcon</default>
        </input>
    </form>
</mixin>
