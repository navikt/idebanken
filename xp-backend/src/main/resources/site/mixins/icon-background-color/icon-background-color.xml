                <option value="bg-white">Hvit</option>
                <option value="bg-pink">Rosa</option>
                <option value="bg-blue">Blå</option>
                <option value="bg-orange">Orange</option>
                <option value="bg-yellow">Gul</option>
                <option value="bg-red">Rød</option>
            </config>
        </input>
    </form>
</mixin>
